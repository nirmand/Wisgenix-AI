      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "AzureAppServicesFile": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "AzureAppServicesBlob": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
